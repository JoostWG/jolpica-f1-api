{
    "lang": "en",
    "cleanOutputDir": true,
    "entryPoints": ["src/index.ts"],
    "out": "./docs",
    "plugin": ["./typedoc-plugin.mjs"],
    "navigation": {
        "includeCategories": true
    },
    "sort": [
        "source-order"
    ],
    "navigationLeaves": ["JSONOutput"],
    "categoryOrder": [
        "Base",
        "Models",
        "Enums",
        "Errors",
        "Options",
        "Api data",
        "Api responses",
        "Other"
    ],
    "sourceLinkExternal": true,
    "excludeInternal": true,
    "router": "category"
}
