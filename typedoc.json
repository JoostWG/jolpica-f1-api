{
    "lang": "en",
    "cleanOutputDir": true,
    "entryPoints": ["src/index.ts"],
    "out": "./docs-build",
    "plugin": ["./typedoc-plugin.mjs"],
    "navigation": {
        "includeCategories": true
    },
    "sort": [
        "source-order"
    ],
    "navigationLeaves": ["JSONOutput"],
    "categoryOrder": [
        "Base",
        "Models",
        "Enums",
        "Errors",
        "Options",
        "Api data",
        "Api responses",
        "Other"
    ],
    "notRenderedTags": [
        "@showCategories",
        "@showGroups",
        "@hideCategories",
        "@hideGroups",
        "@disableGroups",
        "@expand",
        "@preventExpand",
        "@expandType",
        "@summary",
        "@group",
        "@groupDescription",
        "@category",
        "@categoryDescription",
        "@sortStrategy"
    ],
    "sourceLinkExternal": true,
    "excludeInternal": true,
    "router": "category"
}
