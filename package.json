{
    "name": "jolpica-f1-api",
    "version": "2.1.0",
    "description": "API wrapper for the Jolpica F1 API",
    "main": "./dist/index.js",
    "scripts": {
        "test": "ts-node src/test/index.ts",
        "dev": "tsup src/index.ts --watch ./src --dts",
        "build": "tsup ./src --dts --clean --minify --minify-whitespace --minify-identifiers --minify-syntax",
        "lint": "eslint . --fix --cache",
        "format": "dprint fmt",
        "type-check": "tsc --noEmit",
        "quality": "concurrently -n ESLint,dprint,TypeScript -c #3178C6,#4D5564,#4B32C3 \"npm run lint\" \"npm run format\" \"npm run type-check\" ",
        "typedoc": "typedoc src/index.ts"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@stylistic/eslint-plugin": "^5.5.0",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "@typescript-eslint/parser": "^8.46.3",
        "concurrently": "^9.2.1",
        "dprint": "^0.50.2",
        "eslint": "^9.39.1",
        "eslint-plugin-check-file": "^3.3.1",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.0",
        "typedoc": "^0.28.15",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.3"
    },
    "dependencies": {
        "axios": "^1.13.1"
    }
}
